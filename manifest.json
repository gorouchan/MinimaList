{
  "manifest_version": 3,
  "name": "MinimaList: Minimal New Tab Todo List",
  "version": "1.1",
  "description": "Simple and Elegant todo list to replace your 'New Tab' page",
  "author": "Koji",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "unlimitedStorage"],
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
